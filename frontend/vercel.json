{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": false,
      "preview": true
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 