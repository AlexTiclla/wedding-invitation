{
   "builds": [
      { "src": "frontend", "use": "@vercel/static" },
      { "src": "back", "use": "@vercel/node" }
   ],
   "routes": [
      { "src": "/api/(.*)", "dest": "back/$1" },
      { "src": "/(.*)", "dest": "frontend/index.html" }
   ],
   "rewrites": [
      { "source": "/(.*)", "destination": "/frontend/$1" }
   ]
}
